with open('names2.csv', 'w') as csvfile:
    fieldnames = ['X', 'Y']
    writer = csv.DictWriter(csvfile, fieldnames=fieldnames)
    writer.writeheader()
    #print "data is", repr(p)
    for a in p:
    	d, c = get_coordinates1(a)
    	writer.writerow({'X': d, 'Y': c})
